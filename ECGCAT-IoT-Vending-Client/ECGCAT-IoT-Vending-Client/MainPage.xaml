<Page
    x:Class="ECGCAT_IoT_Vending_Client.MainPage"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:ECGCAT_IoT_Vending_Client"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d">

    <Grid Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">
        <TextBlock x:Name="time" HorizontalAlignment="Left" Margin="14,75,0,0" TextWrapping="Wrap" Text="Time:" VerticalAlignment="Top" Height="49" Width="331" FontSize="18.667"/>
        <TextBlock x:Name="temperature" HorizontalAlignment="Left" Margin="14,129,0,0" TextWrapping="Wrap" Text="Temp:" VerticalAlignment="Top" Height="49" Width="331" FontSize="18.667"/>
        <TextBlock x:Name="pressure" HorizontalAlignment="Left" Margin="14,178,0,0" TextWrapping="Wrap" Text="Pressure: " VerticalAlignment="Top" Height="49" Width="331" FontSize="18.667"/>
    </Grid>
</Page>
